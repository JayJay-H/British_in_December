<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="500.0" prefWidth="400.0" stylesheets="@../Css/ScooterManagementGuiCss.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Manager.Java.Controllers.ScooterManagementController">
	<children>
		<ListView fx:id="scooterListView" layoutX="14.0" layoutY="92.0" prefHeight="200.0" prefWidth="375.0" stylesheets="@../Css/ScooterManagementGuiCss.css" />
		<Label layoutX="14.0" layoutY="63.0" stylesheets="@../Css/ScooterManagementGuiCss.css" text="※ 현재 배치된 스쿠터 목록">
         <font>
            <Font size="14.0" />
         </font></Label>
		<Label id="ScooterListLabel" alignment="CENTER" layoutY="-1.0" prefHeight="30.0" prefWidth="400.0" stylesheets="@../Css/ScooterManagementGuiCss.css" text="Scooter List">
         <font>
            <Font size="18.0" />
         </font></Label>
		<Button fx:id="backButton" layoutX="345.0" layoutY="39.0" mnemonicParsing="false" onAction="#backButtonHandler" stylesheets="@../Css/ScooterManagementGuiCss.css" text="Back" />
      <VBox id="FindScooterVBox" layoutX="14.0" layoutY="304.0" prefHeight="78.0" prefWidth="375.0">
         <children>
      		<Label stylesheets="@../Css/ScooterManagementGuiCss.css" text=" ● 조회할 스쿠터의 ID를 입력하세요.">
               <VBox.margin>
                  <Insets bottom="10.0" top="10.0" />
               </VBox.margin>
            </Label>
            <HBox prefHeight="30.0" prefWidth="374.0">
               <children>
            		<Label stylesheets="@../Css/ScooterManagementGuiCss.css" text=" ID : ">
                     <HBox.margin>
                        <Insets bottom="5.0" top="5.0" />
                     </HBox.margin>
                  </Label>
            		<TextField fx:id="scooterCheckID" prefHeight="22.0" prefWidth="206.0" promptText="Scooter의 ID를 입력하세요" stylesheets="@../Css/ScooterManagementGuiCss.css">
                     <HBox.margin>
                        <Insets bottom="5.0" left="45.0" right="10.0" top="5.0" />
                     </HBox.margin>
                  </TextField>
            		<Button fx:id="checkButton" mnemonicParsing="false" onAction="#checkButtonHandler" prefHeight="25.0" prefWidth="70.0" stylesheets="@../Css/ScooterManagementGuiCss.css" text="조회하기">
                     <HBox.margin>
                        <Insets bottom="5.0" left="10.0" top="5.0" />
                     </HBox.margin>
                  </Button>
               </children>
            </HBox>
         </children>
      </VBox>
      <VBox id="AddScooterVBox" layoutX="12.0" layoutY="400.0" prefHeight="94.0" prefWidth="375.0">
         <children>
      		<Label stylesheets="@../Css/ScooterManagementGuiCss.css" text=" ● 추가할 스쿠터의 정보를 입력하세요.">
               <VBox.margin>
                  <Insets bottom="10.0" top="10.0" />
               </VBox.margin>
            </Label>
            <HBox prefHeight="51.0" prefWidth="375.0">
               <children>
                  <VBox prefHeight="35.0" prefWidth="73.0">
                     <children>
                  		<Label stylesheets="@../Css/ScooterManagementGuiCss.css" text=" ID : ">
                           <padding>
                              <Insets bottom="5.0" />
                           </padding>
                        </Label>
                  		<Label stylesheets="@../Css/ScooterManagementGuiCss.css" text=" Location : " />
                     </children>
                     <HBox.margin>
                        <Insets bottom="5.0" top="5.0" />
                     </HBox.margin>
                  </VBox>
                  <VBox prefHeight="45.0" prefWidth="215.0">
                     <children>
                  		<TextField fx:id="scooterID" prefWidth="219.0" promptText="Scooter의 ID를 입력하세요" stylesheets="@../Css/ScooterManagementGuiCss.css">
                           <VBox.margin>
                              <Insets left="5.0" />
                           </VBox.margin>
                        </TextField>
                  		<TextField fx:id="scooterXLocation" prefHeight="22.0" prefWidth="206.0" promptText="x 좌표" stylesheets="@../Css/ScooterManagementGuiCss.css">
                           <VBox.margin>
                              <Insets left="5.0" />
                           </VBox.margin>
                        </TextField>
                     </children>
                  </VBox>
                  <VBox prefHeight="200.0" prefWidth="100.0">
                     <children>
                  		<Button fx:id="addButton" mnemonicParsing="false" onAction="#addButtonHandler" prefHeight="47.0" prefWidth="70.0" stylesheets="@../Css/ScooterManagementGuiCss.css" text="추가">
                           <VBox.margin>
                              <Insets left="15.0" />
                           </VBox.margin>
                        </Button>
                     </children>
                     <HBox.margin>
                        <Insets left="5.0" />
                     </HBox.margin>
                  </VBox>
               </children>
            </HBox>
         </children>
      </VBox>
      <Button id="CloseButton" fx:id="closeButton" layoutX="372.0" layoutY="2.0" mnemonicParsing="false" onAction="#closeButtonHandler" prefHeight="9.0" prefWidth="3.0" text="X" />
	</children>
</AnchorPane>
